<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="ac"
            type="shy.car.sdk.travel.login.ui.ChangeMobileActivity" />

        <variable
            name="presenter"
            type="shy.car.sdk.travel.login.presenter.ChangeMobilePresenter"/>
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <include
            layout="@layout/layout_title"
            app:title="@{@string/str_change_mobile}" />

        <View style="@style/style_shadow" />

        <FrameLayout
            android:id="@+id/frame_change_mobile"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </LinearLayout>
</layout>