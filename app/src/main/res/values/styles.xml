<resources>

    <style name="AppThemeBase" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/main_back</item>
        <item name="colorAccent">@color/text_primary_333333</item>
        <item name="android:colorBackground">@color/main_back</item>
        <item name="android:textColorPrimary">@color/text_primary_333333</item>
        <item name="android:textColorSecondary">@color/text_second_primary_666666</item>
        <item name="android:textColor">@color/text_primary_333333</item>
        <item name="android:windowBackground">@color/main_back</item>
        <item name="android:background">@color/main_back</item>

    </style>


    <style name="AppTheme" parent="AppThemeBase" />

    <style name="line_horizontal">
        <item name="android:background">@color/text_fifth_primary_e5e5e5</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <style name="line_vertical">
        <item name="android:background">@color/text_fifth_primary_e5e5e5</item>
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="line_vertical_cccccc">
        <item name="android:background">#cccccc</item>
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="style_verify_edt">
        <item name="android:layout_width">36dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:background">@drawable/back_verify_edit</item>
        <item name="android:gravity">center</item>
        <item name="android:inputType">number</item>
        <item name="android:maxLength">1</item>
        <item name="android:textColor">@color/main_color_green</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="style_verify_content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="style_user_center_menu">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">30dp</item>
        <item name="android:drawablePadding">13dp</item>

        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/text_primary_333333</item>
        <item name="android:textSize">@dimen/text_title_16dp</item>
    </style>

    <style name="style_mw">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="style_wm">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="style_ww">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="style_mm">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="style_mw.gcw1">
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="style_mm.gcw1">
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="style_mm.w1">
        <item name="android:layout_weight">1</item>
    </style>

    <style name="style_user_center_bottom_content" parent="style_mw.gcw1"/>

    <style name="style_ww.user_center_bottom_icon">
        <item name="android:textColor">#353235</item>
        <item name="android:textSize">@dimen/text_second_primary_12dp</item>
    </style>

    <style name="style_mm.gcw1.home_page_title">
        <item name="android:button">@null</item>
        <item name="android:textSize">@dimen/text_primary_14dp</item>
        <item name="android:textColor">@drawable/color_home_page_title</item>
    </style>

    <style name="style_wm.car">
        <item name="android:button">@null</item>
        <item name="android:textSize">@dimen/text_second_primary_12dp</item>
        <item name="android:background">@drawable/back_under_line_selector</item>
        <item name="android:textColor">@drawable/color_home_filter_title</item>
    </style>

    <style name="style_shadow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@drawable/shadow</item>
    </style>
</resources>
